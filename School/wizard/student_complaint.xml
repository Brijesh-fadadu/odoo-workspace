<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>


        <!-- Form View -->
        <record id="student_complaint_form" model="ir.ui.view">
            <field name="name">student_complaint_form</field>
            <field name="model">student_complaint</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                             <field name="name"/>
                            <field name="email"/>
                            <field name="complaint"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>


    </data>
</odoo>
